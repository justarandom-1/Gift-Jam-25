.test {
    background-image: resource('Cutscenes/sb1');
}

.test2 {
    position: absolute;
    top: 50%;
    -unity-font-definition: resource('Cutscenes/KaiseiDecol-Bold');
    font-size: 60px;
    color: rgb(27, 215, 27);
    -unity-paragraph-spacing: -10px;
    letter-spacing: 5px;
}

.yarnspinner {
    position: absolute;
}

.spinning {
    height: 23%;
    width: 75%;
    top: 70%;
    left: 12.5%;
    right: 12.5%;
    background-color: rgba(94, 91, 91, 0.47);
    border-top-left-radius: 60px;
    border-top-right-radius: 60px;
    border-bottom-right-radius: 60px;
    border-bottom-left-radius: 60px;
    font-size: 40px;
    -unity-font-definition: url("project://database/Assets/TextMesh%20Pro/Examples%20&%20Extras/Fonts/Electronic%20Highway%20Sign.TTF?fileID=12800000&guid=8a2b9e2a607dd2143b58c44bc32410b4&type=3#Electronic Highway Sign");
    margin-top: 6px;
    -unity-paragraph-spacing: 0;
    padding-top: 35px;
    padding-right: 2px;
    padding-left: 35px;
    letter-spacing: 2px;
    word-spacing: 0;
    text-shadow: 0 0 0 rgb(0, 0, 0);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
}
